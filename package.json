{
  "private": true,
  "name": "value-creator",
  "description": "",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "rimraf public/build && remix build",
    "build:tailwind": "tailwindcss --minify -o app/styles/tailwind.css",
    "deploy": "npm run build && wrangler publish",
    "dev": "NODE_ENV=development remix build && run-p dev:*",
    "dev:miniflare": "NODE_ENV=development miniflare ./build/index.js --watch",
    "dev:remix": "remix watch",
    "dev:tailwind": "tailwindcss -o app/styles/tailwind.css --watch",
    "format": "prettier **/* --write",
    "lint": "eslint .",
    "start": "NODE_ENV=production miniflare ./build/index.js"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.6.7",
    "@remix-run/cloudflare-workers": "^1.6.7",
    "@remix-run/react": "^1.6.7",
    "canvas-confetti": "^1.4.0",
    "date-fns": "^2.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.7.1",
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@types/canvas-confetti": "^1.4.2",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "autoprefixer": "^10.0.2",
    "esbuild": "0.13.14",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "miniflare": "2.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false
}
