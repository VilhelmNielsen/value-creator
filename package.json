{
  "private": true,
  "name": "value-creator",
  "description": "",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "rimraf public/build && remix build",
    "build:tailwind": "tailwindcss --minify -o app/styles/tailwind.css",
    "deploy": "npm run build && wrangler publish",
    "dev": "NODE_ENV=development remix build && run-p dev:*",
    "dev:miniflare": "NODE_ENV=development miniflare ./build/index.js --watch",
    "dev:remix": "remix watch",
    "dev:tailwind": "tailwindcss -o app/styles/tailwind.css --watch",
    "format": "prettier **/* --write",
    "lint": "eslint .",
    "start": "NODE_ENV=production miniflare ./build/index.js",
    "test:e2e": "playwright test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.9.0",
    "@remix-run/cloudflare-workers": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "canvas-confetti": "^1.6.0",
    "date-fns": "^2.29.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.14.1",
    "@playwright/test": "^1.29.1",
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/canvas-confetti": "^1.6.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "autoprefixer": "^10.4.13",
    "esbuild": "0.15.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.3",
    "miniflare": "2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false
}
